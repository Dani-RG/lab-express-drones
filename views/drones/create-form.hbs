<h2>Create new drone</h2>
<form method="POST" action="/drones/create">
    <label>Name</label>
    <input required type="text" name="name"/>
    <label>Propellers</label>
    <input required type="number" name="propellers"/>
    <label>Maximun speed</label>
    <input required type="number" name="maxSpeed"/>
    <button type="submit">Create</button>
</form>